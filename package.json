{
  "name": "yukepo.com-v4-front-end",
  "version": "1.0.0",
  "description": "Yukepo.com version 4",
  "main": "index.js",
  "scripts": {
    "dev": "set NODE_ENV=development && node ./local/server",
    "dev:build:client": "webpack --config webpack.client.config.js",
    "dev:build:server": "webpack --config webpack.server.config.js",
    "dev:build": "npm-run-all dev:build:*",
    "prod": "set NODE_ENV=production && node ./dist/server",
    "prod:build:client": "webpack --config webpack.production-client.config.js",
    "prod:build:server": "webpack --config webpack.production-server.config.js",
    "prod:build": "npm-run-all prod:build:*",
    "pm2": "set NODE_ENV=production && pm2 start ./dist/server"
  },
  "keywords": [],
  "author": "Loris Tujiba",
  "license": "ISC",
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.4",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "npm-run-all": "^4.1.3",
    "webpack": "^4.6.0",
    "webpack-cli": "^2.0.14",
    "webpack-dev-middleware": "^3.1.3",
    "webpack-hot-middleware": "^2.22.2",
    "webpack-node-externals": "^1.7.2"
  },
  "dependencies": {
    "axios": "^0.18.0",
    "compression": "^1.7.3",
    "css-loader": "^1.0.0",
    "dotenv": "^5.0.1",
    "express": "^4.16.3",
    "file-loader": "^1.1.11",
    "global": "^4.3.2",
    "react": "^16.3.2",
    "react-dom": "^16.3.2",
    "react-helmet": "^5.2.0",
    "react-hot-loader": "^4.1.3",
    "react-owl-carousel2": "^0.3.0",
    "react-redux": "^5.0.7",
    "react-redux-loading-bar": "^4.0.5",
    "react-router-dom": "^4.2.2",
    "redux": "^4.0.0",
    "redux-thunk": "^2.2.0",
    "serialize-javascript": "^1.5.0",
    "style-loader": "^0.21.0",
    "styled-components": "^3.2.6",
    "url-loader": "^1.0.1"
  }
}
